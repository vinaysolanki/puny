<!DOCTYPE html>
<html>
<head>
  <title>Puny</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <meta name=”description” content=”Puny is a simple URL shortener that helps people share news, stories and articles quickly without any hassle of signing up or logging in.”/>
  <meta name=”keywords” content=”url shortener,url compress, share quickly, easy url shortener, simple url shortener" />
  <meta property="og:title" content="Puny" />
  <meta property="og:type" content="website" />
  <meta property="og:url" content="http://pn.ee" />
  <meta property="og:image" content="http://pn.ee/assets/Puny-logo2.png" />
  <meta property="og:site_name" content="Puny - A Simple URL Shortener" />
  <meta property="fb:admins" content="100000194351905" />
  
</head>
<body>

<!-- Google Analytics Code -->
<script type="text/javascript">

  var _gaq = _gaq || [];
  _gaq.push(['_setAccount', 'UA-39468545-1']);
  _gaq.push(['_trackPageview']);

  (function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
  })();

</script>
<!--#######################-->


<!-- Facebook SDK -->

<div id="fb-root"></div>
<script>(function(d, s, id) {
  var js, fjs = d.getElementsByTagName(s)[0];
  if (d.getElementById(id)) return;
  js = d.createElement(s); js.id = id;
  js.src = "//connect.facebook.net/en_US/all.js#xfbml=1";
  fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));</script>

<!--#########################-->


<!-- Google button script -->

<!--######################-->


<!-- Twitter button script -->

<script>!function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0];if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src="//platform.twitter.com/widgets.js";fjs.parentNode.insertBefore(js,fjs);}}(document,"script","twitter-wjs");
</script>

<!--########################-->


<!-- Feedback widget -->
<!-- UserVoice JavaScript SDK (only needed once on a page) -->
<script>(function(){var uv=document.createElement('script');uv.type='text/javascript';uv.async=true;uv.src='//widget.uservoice.com/vASCezSSWZpkFGSG98A.js';var s=document.getElementsByTagName('script')[0];s.parentNode.insertBefore(uv,s)})()</script>

<!-- A tab to launch the Classic Widget -->
<script>
UserVoice = window.UserVoice || [];
UserVoice.push(['showTab', 'classic_widget', {
  mode: 'full',
  primary_color: '#cc6d00',
  link_color: '#007dbf',
  default_mode: 'feedback',
  forum_id: 198817,
  tab_label: 'Feedback',
  tab_color: '#cc6d00',
  tab_position: 'middle-right',
  tab_inverted: false
}]);
</script>

<!--#########################-->

<%= yield %>

</body>
</html>
